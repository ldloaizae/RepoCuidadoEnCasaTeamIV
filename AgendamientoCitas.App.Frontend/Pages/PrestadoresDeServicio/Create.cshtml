@page
@using AgendamientoCitas.App.Dominio
@model AgendamientoCitas.App.Frontend.Pages.PrestadoresDeServicio.CreateModel
@{
}
<h1>Nuevo Prestador de Servicio</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="prestadorDeServicio.RazonSocial"></label>
        <input asp-for="prestadorDeServicio.RazonSocial" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="prestadorDeServicio.Nit"></label>
        <input asp-for="prestadorDeServicio.Nit" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="prestadorDeServicio.TipoDePrestador"></label>
        <select asp-for="prestadorDeServicio.TipoDePrestador" asp-items="Html.GetEnumSelectList<TipoDePrestador>()" class="form-control">
        </select>
    </div>
    <div class="form-group">
        <label asp-for="prestadorDeServicio.Telefono"></label>
        <input asp-for="prestadorDeServicio.Telefono" class="form-control">
    </div>
    <button type="submit" class="btn btn-primary">Crear prestador de servicio</button>
</form>
<a class="btn btn-secundary" asp-page="./Index">Volver</a>