@page
@model AgendamientoCitas.App.Frontend.Pages.Citas.DetailsModel
@{
}
<h1>Detalles de la Cita</h1>
<table class="table table-dark">
    <tr>
        <th scope="col">Id</th>
        <td>@Model.cita.Id</td>
    </tr>
    <tr>
        <th scope="col">Hora</th>
        <td>@Model.cita.FechaHora</td>
    </tr>
    <tr>
        <th scope="col">Tipo de Servicio</th>
        <td>@Model.cita.TipoServicio</td>
    </tr>
    <tr>
        <th scope="col">Consultorio</th>
        <td>@Model.cita.Consultorio</td>
    </tr>
    <tr>
        <th scope="col">Duracion</th>
        <td>@Model.cita.Duracion</td>
    </tr>
    <tr>
        <th scope="col">Valor</th>
        <td>@Model.cita.Valor</td>
    </tr>
</table>
<a class="btn btn-primary" asp-page="./Index">Volver</a>