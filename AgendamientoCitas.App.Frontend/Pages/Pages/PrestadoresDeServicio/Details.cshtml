@page
@model AgendamientoCitas.App.Frontend.Pages.PrestadoresDeServicio.DetailsModel
@{
}

<h1>Detalles del pacientes</h1>
<table class="table table-dark">
    <tr>
        <th scope="col">Id</th>
        <td>@Model.prestadoreDeServicio.Id</td>
    </tr>
    <tr>
        <th scope="col">Razon Social</th>
        <td>@Model.prestadoreDeServicio.RazonSocial</td>
    </tr>
    <tr>
        <th scope="col">Nit</th>
        <td>@Model.prestadoreDeServicio.Nit</td>
    </tr>
    <tr>
        <th scope="col">Tipo Prestador de Servicio</th>
        <td>@Model.prestadoreDeServicio.TipoDePrestador</td>
    </tr>
    <tr>
        <th scope="col">Telefono</th>
        <td>@Model.prestadoreDeServicio.Telefono</td>
    </tr>
</table>
<a class="btn btn-primary" asp-page="./Index">Volver</a>